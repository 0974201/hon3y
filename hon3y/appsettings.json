{

  "Kestrel": {
    "Endpoints": {
      "Test": {
        "Url": "https://localhost:8080"
      }
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning"

      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".test.txt",
          "rollingInterval": "Hour"
        }
      }
    ]
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Data Source=./Data/TestDB.db"
    //"DefaultConnection": "Data Source=localhost;Integrated Security=True;",
    //"DbConnection": "Data Source=localhost;Initial Catalog=TestDB1;Integrated Security=True;"
    //"DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=aspnet-53bc9b9d-9d6a-45d4-8429-2a2761773502;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
