@page
@model hon3y.Pages.FormModel
@{
    ViewData["Title"] = "Formulier";
}

<div>
    <form method="post">
        <label for ="voornaam">test</label>
        <input for="voornaam" type="text" placeholder="voornaam" /><br />
        <label for="achternaam">test</label>
        <input for="achternaam" type="text" placeholder="achternaam" /><br />
        <label for="email">test</label>
        <input for="email" type="email" placeholder="email" /><br />
        <label for="telefoonnummer">test</label>
        <input for="telefoonnummer" type="number" placeholder="telefoonnummer" /><br />
        <label for="afspraakreden">test</label>
        <input for="afspraakreden" type="text" placeholder="reden afspraak" /><br />
        <label for="datum">test</label>
        <input for="datum" type="datetime-local" value="@DateTime.Now.ToString("dd-MM-yyyy HH:mm")" /><br />
        <button>Verzend</button>
    </form>
</div>